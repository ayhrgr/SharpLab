{
  "name": "sharplab",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ts-node-script ./scripts.ts build",
    "lint": "ts-node-script ./scripts.ts ts:lint",
    "test": "jest",
    "test:skip-render": "cross-env TEST_SKIP_RENDER=true jest --testPathIgnorePatterns rendering",
    "start": "ts-node-script ./scripts.ts start --watch",
    "build-ci": "ts-node-script ./scripts.ts build-ci"
  },
  "engines": {
    "node": ">=16.14.2"
  },
  "jest": {
    "maxConcurrency": 10,
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tests/tsconfig.json"
      }
    },
    "testEnvironment": "jsdom",
    "transform": {
      "\\.ts$": "ts-jest",
      "(node_modules[/\\\\]|mirrorsharp.+dist)": "./tests/transform-esm-in-node-modules.cjs"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(?:mirrorsharp|node-fetch|data-uri-to-buffer|fetch-blob|formdata-polyfill))"
    ],
    "setupFiles": [
      "<rootDir>/ts/polyfills/index.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "testMatch": [
      "<rootDir>/app/**/*.tests.ts",
      "<rootDir>/tests/**/*.tests.ts"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "7.9.0",
    "@peculiar/webcrypto": "1.1.7",
    "@types/adm-zip": "0.4.33",
    "@types/applicationinsights-js": "1.0.9",
    "@types/autoprefixer": "9.7.2",
    "@types/codemirror": "0.0.78",
    "@types/css-font-loading-module": "0.0.4",
    "@types/d3": "5.7.2",
    "@types/d3-force": "1.2.1",
    "@types/dateformat": "5.0.0",
    "@types/html-minifier": "3.5.3",
    "@types/jest": "27.4.1",
    "@types/jest-image-snapshot": "4.3.1",
    "@types/less": "3.0.3",
    "@types/lz-string": "1.3.33",
    "@types/marked": "4.0.3",
    "@types/node": "16.11.4",
    "@types/node-fetch": "2.6.1",
    "@types/proper-lockfile": "4.1.2",
    "@types/puppeteer": "5.4.5",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.0",
    "@types/sharp": "0.30.1",
    "@types/throttle-debounce": "2.1.0",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "adm-zip": "0.5.1",
    "autoprefixer": "10.4.4",
    "babel-jest": "27.5.1",
    "cross-env": "7.0.3",
    "esbuild": "0.14.34",
    "eslint": "8.12.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react-hooks": "4.4.0",
    "execa": "5.0.0",
    "fs-jetpack": "2.1.0",
    "html-minifier": "3.5.19",
    "http-server": "14.1.0",
    "jest": "27.5.1",
    "jest-image-snapshot": "4.5.1",
    "jest-mock-proxy": "3.0.0",
    "less": "4.1.2",
    "node-fetch": "3.2.3",
    "oldowan": "1.0.0",
    "postcss": "8.4.12",
    "postcss-csso": "6.0.0",
    "proper-lockfile": "4.1.2",
