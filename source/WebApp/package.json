    "codemirror": "5.61.1",
    "codemirror-addon-infotip": "1.0.0",
    "codemirror-addon-lint-fix": "1.0.1",
    "d3": "4.13.0",
    "d3-force": "2.0.0",
    "dateformat": "5.0.3",
    "lz-string": "1.4.4",
    "marked": "4.0.13",
    "mirrorsharp": "file:../%23external/mirrorsharp/WebAssets/dist",
    "mirrorsharp-codemirror-6-preview": "0.13.0",
    "normalize.css": "8.0.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "recoil": "0.7.2",
